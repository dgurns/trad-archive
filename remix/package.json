{
	"private": true,
	"sideEffects": false,
	"scripts": {
		"dev:css": "tailwindcss -w -i ./app/styles/globals.css -o ./app/styles/globals-generated-do-not-edit.css",
		"dev:remix": "remix watch",
		"dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --binding $(cat .env)",
		"dev": "remix build && run-p dev:*",
		"start": "cross-env NODE_ENV=production npm run dev:wrangler",
		"build:css": "tailwindcss -m -i ./app/styles/globals.css -o ./app/styles/globals-generated-do-not-edit.css",
		"build": "npm run build:css && remix build"
	},
	"prisma": {
		"seed": "node_modules/ts-node/dist/bin.js prisma/seed.ts"
	},
	"dependencies": {
		"@heroicons/react": "^1.0.6",
		"@prisma/client": "3.15.0",
		"@remix-run/cloudflare": "^1.5.1",
		"@remix-run/cloudflare-pages": "^1.5.1",
		"@remix-run/react": "^1.5.1",
		"@tailwindcss/forms": "^0.5.2",
		"cross-env": "^7.0.3",
		"prisma": "^3.15.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^3.10.0",
		"@remix-run/dev": "^1.5.1",
		"@remix-run/eslint-config": "^1.5.1",
		"@types/react": "^17.0.45",
		"@types/react-dom": "^17.0.17",
		"autoprefixer": "^10.4.7",
		"concurrently": "^7.2.0",
		"eslint": "^8.15.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.14",
		"tailwindcss": "^3.0.24",
		"ts-node": "^10.8.1",
		"typescript": "^4.6.4",
		"wrangler": "beta"
	},
	"engines": {
		"node": ">=14"
	}
}
